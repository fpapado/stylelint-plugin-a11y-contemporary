{
  "name": "stylelint-plugin-a11y-contemporary",
  "version": "2500.0.0",
  "description": "Stylelint plugin for better accessibility, contemporary to codebases written in 2025 and beyond.",
  "keywords": [
    "stylelint",
    "stylelint-plugin"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/fpapado/stylelint-plugin-a11y-contemporary.git"
  },
  "license": "MIT",
  "author": "Fotis Papadogeorgopoulos",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "docs"
  ],
  "scripts": {
    "build": "tsc --noCheck",
    "check-types": "tsc --noEmit",
    "check-types:watch": "tsc --noEmit --watch",
    "format": "pnpm prettier --write .",
    "lint": "npm-run-all \"lint:*\"",
    "lint:prettier": "pnpm prettier --check .",
    "lint:scripts": "eslint",
    "prepublishOnly": "pnpm build && publint && attw --pack",
    "publint": "publint",
    "test": "vitest"
  },
  "dependencies": {
    "postcss": "^8.5.3"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "0.17.4",
    "@eslint/js": "9.21.0",
    "@tsconfig/node22": "22.0.0",
    "@types/node": "22.13.5",
    "dedent": "1.5.3",
    "eslint": "9.21.0",
    "jiti": "2.4.2",
    "npm-run-all2": "7.0.2",
    "prettier": "3.5.2",
    "prettier-plugin-packagejson": "2.5.8",
    "publint": "0.3.6",
    "stylelint": "16.14.1",
    "typescript": "5.7.3",
    "typescript-eslint": "8.24.1",
    "vitest": "3.0.6",
    "vitest-stylelint-utils": "^1.1.0"
  },
  "peerDependencies": {
    "stylelint": "^16.0.0"
  },
  "packageManager": "pnpm@10.4.1+sha512.c753b6c3ad7afa13af388fa6d808035a008e30ea9993f58c6663e2bc5ff21679aa834db094987129aa4d488b86df57f7b634981b2f827cdcacc698cc0cfb88af",
  "engines": {
    "node": "^20 || ^22 || ^23"
  },
  "publishConfig": {},
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  }
}
